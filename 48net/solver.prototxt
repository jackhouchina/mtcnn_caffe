train_net : "train48.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
stepsize: 300000
gamma: 0.1
display: 200   #500
max_iter: 1200000
snapshot: 20000
snapshot_prefix: "ONet"
solver_mode: GPU
type: "SGD"
